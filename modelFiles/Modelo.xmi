      <opciones valor="8 años o mas"/>
    </preguntas>
    <preguntas xsi:type="encuesta2:Multiple" enunciado="Estudio en una universidad?">
      <opciones valor="Publica"/>
      <opciones valor="Privada"/>
      <opciones valor="Privada y publica"/>
    </preguntas>
    <preguntas xsi:type="encuesta2:Unica" enunciado="Usted termino todos sus estudios?">
      <opciones valor="si"/>
      <opciones valor="no"/>
    </preguntas>
    <preguntas xsi:type="encuesta2:Unica" enunciado="Usted Pago?">
      <opciones valor="Menos de un millon"/>
      <opciones valor="Mas de un millon y menos de 4 millones"/>
      <opciones valor="Mas de 4 millones"/>
    </preguntas>
    <preguntas xsi:type="encuesta2:Abierta" enunciado="Cual es el nombre de su universidad?"/>
    <nodos xsi:type="encuesta2:NodoInicio" texto="Inicio Mod universitario"/>
