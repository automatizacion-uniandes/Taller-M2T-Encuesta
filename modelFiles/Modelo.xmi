    <preguntas xsi:type="encuesta2:Unica" enunciado="Usted es trabajador independiente?">
      <opciones valor="si."/>
      <opciones valor="no."/>
    </preguntas>
    <preguntas xsi:type="encuesta2:Multiple" enunciado="Su/s contrato/s es/son">
